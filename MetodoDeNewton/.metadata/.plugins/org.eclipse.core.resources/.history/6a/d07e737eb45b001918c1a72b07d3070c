import javax.swing.JOptionPane;
public class CMain {
	public static void main (String[] args) {
		String opcao;
		opcao = JOptionPane.showInputDialog(null, "Digite a expressão: ");
		
		for(int i = -100; i <=100; i=i+2) {
			opcao.replaceAll("x", "*" + i);
			
			
		}
		System.out.println(opcao);
	}
}
